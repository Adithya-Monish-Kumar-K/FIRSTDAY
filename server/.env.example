# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key
# Service key bypasses RLS - get this from Project Settings > API > service_role
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# OAuth (Optional - for Google login etc.)
OAUTH_CLIENT_ID=your_oauth_client_id
OAUTH_SECRET_ID=your_oauth_secret_id

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_min_32_chars_recommended
JWT_ALGORITHM=HS256
JWT_EXPIRY_DAYS=7

# Email (SMTP) Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Google Maps API
GOOGLE_MAP_API=your_google_maps_api_key

# Server Port
PORT=5000

# Python Optimizer Service URL
OPTIMIZER_URL=http://localhost:8000

# Node Environment
NODE_ENV=development
